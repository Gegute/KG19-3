<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/textViewInput"
        android:layout_width="120dp"
        android:layout_height="300dp"
        android:background="#873600"
        />

    <TextView
        android:id="@+id/textView"
        android:layout_width="120dp"
        android:layout_height="300dp"
        android:background="#E59866"
        android:layout_toRightOf="@+id/textViewInput"
        />
    <TextView
        android:id="@+id/text"
        android:layout_width="120dp"
        android:layout_height="300dp"
        android:background="#fbca6e"
        android:layout_toRightOf="@+id/textView"
        />
    <TextView
        android:id="@+id/textViewInput2"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:background="#873600"
        android:layout_below="@+id/textViewInput"
        />
    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:background="#E59866"
        android:layout_below="@+id/textViewInput2"
        />
    <TextView
        android:id="@+id/text2"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:background="#fbca6e"
        android:layout_below="@+id/textView2"
        />
    <TextView
        android:id="@+id/line"
        android:layout_width="420dp"
        android:layout_height="8dp"
        android:layout_above="@+id/textViewInput2"
        android:background="#000000" />

    <TextView
        android:id="@+id/line2"
        android:layout_width="420dp"
        android:layout_height="8dp"
        android:layout_below="@+id/text2"
        android:background="#000000" />


</RelativeLayout>